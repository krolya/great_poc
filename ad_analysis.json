{
  "name": "my_schema",
  "schema": {
    "type": "object",
    "properties": {
      "records": {
        "type": "array",
        "description": "A list of records containing responses to the ads.",
        "items": {
          "type": "object",
          "properties": {
            "Persona": {
              "type": "array",
              "description": "ID of the persona answering the questions.",
              "items": {
                "type": "string"
              }
            },
            "Ad name": {
              "type": "string",
              "description": "Name of the advertisement for further grouping of tests."
            },
            "Response test ID": {
              "type": "string",
              "description": "Unique identifier for the test."
            },
            "fstInterest": {
              "type": "number"
            },
            "fstClarity": {
              "type": "number"
            },
            "fstLikeability": {
              "type": "number"
            },
            "fstLikeabilityExplanation": {
              "type": "string"
            },
            "fstUnique": {
              "type": "number"
            },
            "fstActual": {
              "type": "number"
            },
            "fstTrust": {
              "type": "number"
            },
            "fstMore": {
              "type": "number"
            },
            "fstUse": {
              "type": "number"
            },
            "fstShare": {
              "type": "number"
            },
            "fstMessage": {
              "type": "string"
            }
          },
          "required": [
            "Persona",
            "Ad name",
            "Response test ID",
            "fstInterest",
            "fstClarity",
            "fstLikeability",
            "fstLikeabilityExplanation",
            "fstUnique",
            "fstActual",
            "fstTrust",
            "fstMore",
            "fstUse",
            "fstShare",
            "fstMessage"
          ],
          "additionalProperties": false
        }
      }
    },
    "required": [
      "records"
    ],
    "additionalProperties": false
  },
  "strict": true
}